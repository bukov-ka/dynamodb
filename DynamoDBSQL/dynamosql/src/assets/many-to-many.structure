Table users as U {
  id int [pk, increment] // auto-increment
  name varchar
  email varchar

  Indexes {
    id [unique]
  }
}


Table groups as G {
  id int [pk, increment] // auto-increment
  name varchar

  Indexes {
    id [unique]
  }
}

Table u2g as UG{
  user_id int [pk]
  group_id int [pk]
}

Ref: UG.group_id > G.id
Ref: UG.user_id > U.id